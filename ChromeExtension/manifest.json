{
    "name": "Todolist",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery-2.1.4.min.js", "todos.js"]
        }
    ],

    "permissions": [ "storage" ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "ToDos" 
    }
}


